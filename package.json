{
  "catalog": {
    "@t3-oss/env-core": "0.13.10",
    "@types/bun": "1.3.5",
    "@types/react": "~19.2.7",
    "drizzle-orm": "0.45.1",
    "react": "19.2.3",
    "typescript": "5.9.3",
    "zod": "4.3.5"
  },
  "commitlint": {
    "extends": [
      "@seawatts/commitlint-config"
    ]
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@commitlint/cli": "20.3.0",
    "@seawatts/commitlint-config": "workspace:*",
    "@types/bun": "1.3.5",
    "knip": "5.79.0",
    "lefthook": "2.0.13",
    "turbo": "^2.7.2",
    "typescript": "5.9.3",
    "vitest": "4.0.16"
  },
  "engines": {
    "node": ">=20.12.0"
  },
  "name": "@seawatts/avoid-root",
  "packageManager": "bun@1.2.23",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "git clean -xdf node_modules .turbo",
    "clean:ws": "turbo run clean",
    "cli": "turbo run start --filter=@seawatts/cli",
    "dev": "turbo watch dev",
    "format": "biome check",
    "format:fix": "biome check --write",
    "lint:ws": "bunx sherif@latest",
    "postinstall": "bun lint:ws",
    "test": "bun test",
    "typecheck": "turbo run typecheck"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "bun",
    "esbuild",
    "lefthook"
  ],
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*",
      "tooling/*"
    ]
  }
}
