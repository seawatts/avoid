{
  "compilerOptions": {
    "checkJs": false,
    "jsx": "react-native",
    "moduleSuffixes": [".ios", ".android", ".native", ""],
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "exclude": ["node_modules"],
  "extends": ["@seawatts/tsconfig/base.json"],
  "include": [
    "src",
    "*.ts",
    "*.js",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "global.d.ts"
  ]
}
