{
  "$schema": "https://turborepo.org/schema.json",
  "concurrency": "20",
  "globalEnv": ["NODE_ENV"],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "npm_lifecycle_event",
    "TURBO_TOKEN",
    "TURBO_TEAM",
    "NPM_TOKEN",
    "OPENAI_API_KEY"
  ],
  "tasks": {
    "//#clean": {
      "cache": false
    },
    "build": {
      "dependsOn": ["^build"],
      "env": ["OPENAI_API_KEY"],
      "outputs": ["dist/**", "bin/**"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false,
      "env": ["OPENAI_API_KEY"],
      "persistent": true
    },
    "start": {
      "cache": false,
      "env": ["OPENAI_API_KEY"],
      "persistent": true
    },
    "test": {
      "cache": false,
      "dependsOn": ["^build"],
      "outputs": ["coverage/**"]
    },
    "typecheck": {
      "outputs": ["node_modules/.cache/tsbuildinfo.json"]
    }
  },
  "ui": "tui"
}
